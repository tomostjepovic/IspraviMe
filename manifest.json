{
	"name": "Ispravi.me",
	"version": "1.0",
	"description": "Ispravi me!",
	"permissions": ["storage", "activeTab", "http://*/"],
	"background": { },
	"content_scripts": [
		{
			"matches": ["file://*"],
			"js": ["jQuery/jquery-3.3.1.min.js", "jQuery/jquery-ui.js", "scripts/contentScript-error-modal.js", "scripts/contentScript.js"],
			"css": ["jQuery/jquery-ui.css", "css/main.css"],
			"run_at": "document_end"
		}
	],
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "images/icon.png"
	}
}
